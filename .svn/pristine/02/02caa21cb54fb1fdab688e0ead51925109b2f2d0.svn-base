*{
	padding: 0;
	margin: 0;
}
.clear{
	clear: both;
}
body{
	font-family: helvetica,"microsoft yahei";
	color: #333;
	background: #FAFAFA;
}
.head{
	height:50px;
	background: #D13600;
	width: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 101;
}
.headNav{
	width: 50%;
	height: 30px;
	box-sizing: border-box;
	border: 1px solid #FFFFFF;
	border-radius: 4px;
	position:absolute;
    top:50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    line-height: 30px;
    font-size: 14px;
}
.headNavCover{
	width: 100%;
	height: 100%;
	border-radius: 4px;
	padding: 1px;
	position:absolute;
    top:-1px;
    left: -1px;
    background: #D13600;
    opacity: 0.5;
    display: none;
}
.headNavL{
	width: 50%;
	height: 100%;
	background: #fff;
	float: left;
	text-align: center;
	border-radius: 3px 0 0 3px !important;
	color: #D13600;
}
.headNavR{
	width: 50%;
	height: 100%;
	float: left;
	background: #D13600;
	border-radius:0 3px 3px 0 !important;
	text-align: center;
	color: #fff;
}
.return{
	position: absolute;
	height: 40px;
	top: 6px;
	line-height: 40px;
	font-size: 16px;
	color: #fff;
}
.returnImg{
	position: relative;
    top: 5px;
    display: inline-block;
    margin-left: 3px;
    width: 24px;
    height: 24px;
    background-repeat: no-repeat;
    background-image: url(/resources/images/returnWhite.png);
    background-size: 24px 24px;
}
.tapDiv{
	width: 100%;
	height: 10vw;
	line-height: 10vw;
	font-size: 3vw;
	text-align: center;
	position: fixed;
	top: 50px;
	left: 0;
	box-sizing: border-box;
	z-index: 101;
	background:#FAFAFA;
}
.tap{
	width: 100%;
	height: 100%;
	box-sizing: border-box;
	border-bottom: 1px solid #EBECEE;
}
#tap1{
	border-bottom:2px solid #D13600;
}
.goodsList{
	/* border-bottom: 1px solid #EBECEE; */
	background:#FAFAFA !important;
}
.oneGoods{
	width: 100%;
	position: relative;
	background: #FAFAFA;
}
.goodsImgDiv{
	width: 22vw;
	margin-left: 3vw;
	float: left;
	height: 28vw;
}
.goodsImg{
	width: 22vw;
	height: 22vw;
	display: block;
	border-radius: 3px;
	margin-top: 3vw;
}
.goodsInfo{
	margin-left: 3vw;
	width: 72vw;
	height: 28vw;
	float: left;
	position: relative;
}
.goodsName{
	margin-top: 4vw;
	width: 100%;
	-webkit-line-clamp: 2;
	text-overflow: ellipsis;
	overflow: hidden;
	-webkit-box-orient: vertical;
	display: -webkit-box;
	font-size: 4vw;
}
.goodsInfoFloor{
	position: absolute;
	left: 0;
	bottom:4vw;
	font-size: 3.5vw;
}
.price{
	color: #D13600;
}
.blank{
	width: 100%;
	height:calc(50px + 10vw);
}
.orderInfoDiv{
	width: 95vw;
	margin-left: 5vw;
	color: #999999;
	font-size: 3.5vw;
	height: 8vw;
	line-height: 8vw;
}
.orderNum{
	float: left;
}
.orderTime{
	float: right;
	margin-right: 5vw;
}
.orderStateDiv{
	background:#FFFFFF;
	padding:2vw 0;
	border-top:1px solid #EBECEE;
}
.orderState{
	float: right;
	margin-right: 5vw;
	color: #FBBE88;
	font-size: 4vw;
	line-height:8vw;
}
.blankState{
	width: 100vw;
	height: 100vh;
	position: absolute;
	top: 0;
	left: 0;
	background: #FAFAFA;
	z-index: 100;
}
.blankImgDiv{
	width: 80vw;
	margin-left: 10vw;
	margin-top: 40vw;
}
.blankImg{
	width: 100%;
	display: block;
}
.blankText{
	width: 100%;
	text-align: center;
	font-size: 16px;
	margin-top: 5vw;
	color: #B0B9C2;
}
.blankBtnDiv{
	width: 100%;
	height: 40px;
	position: relative;
	margin-top: 8vw;
	text-align: center;
}
.blankBtn{
	outline: none;
	border: none;
	font-size: 16px;
	border-radius: 20px;
	padding: 0 30px;
	color: #FFFFFF;
	background: #D13600;
	height: 40px;
}
.orderBtn{
	font-size:3.7vw;
	height:8vw;
	outline:none;
	border:1px solid #BFBFBF;
	border-radius:5vw;
	padding:0 5vw;
	background:#FFFFFF;
	margin-left:2vw;
}


.overlay{
	position:fixed;
	_position:absolute;
	width:100%;
	height:100%;
	left:0;
	top:0; 
	/*display: none;*/
    background-color:rgba(0,0,0,0.7);
    z-index:9990; 
}
.popup{
	display: none;
   position: fixed;
   _position: absolute;
   width: 80%;
   background: #FFFFFF;
   outline: none;
   border-radius: 3px;
   background-color: white; 
   z-index:9999; 
   top:50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  padding: 5vw 0;
}


.changePriceDiv{
	margin-left: 5vw;
}
.changeText{
	font-size: 14px;
}
.changeInput{
	float:left;
	margin-right: 3vw;
	width:40vw;
	font-size:14px;
	height: 9vw;
	box-sizing: border-box;
	border: 1px solid #D13600;
	text-align: center;
}
.changeInput::-webkit-input-placeholder{
	text-align: center;
}
.priceIcon{
	float: left;
	line-height: 9vw;
	font-size: 3.4vw;
	margin-right:5px;
}
.changeBtn{
	padding:0 !important;
	margin:0 !important;
	width: 70vw;
	height: 10vw;
	background: #D13600 !important;
	border: none;
	outline: none;
	font-size: 14px !important;
	color: #FFFFFF !important;
	margin-left:5vw !important;
	margin-top:5vw !important;
}

.closeImgDiv{
	position:absolute;
	width:30px;
	height:30px;
	font-size:0;
	top:5px;
	right:5px;
}
.closePopup{
	width:20px;
	height:20px;
	margin:5px;
}

.zhezhao{
	width:100%;
	height:100%;
	position:fixed;
	background:rgba(0,0,0,0.5);
	z-index:999;
	display:none;
}
.trackingNo{
	width:100%;
	height:100%;
	position:fixed;
	top:0;
	left:0;
	z-index:1000;
	background:#fff;
	font-size:14px;
	display:none;
}
.trackingNo .title{
	width:100%;
	background:#D13600;
	color:#fff;
	height:50px;
	line-height:50px;
	text-align:center;
	font-size:16px;
}
.trackingNo .content{
	width:100%;
	height:170px;
	position:relative;
	padding:15px 0;
}
.trackingNo .contentdiv{
	width:100%;
	height:35px;
	line-height:35px;
	position:relative;
}
.contentdiv div{
	width:20%;
	height:35px;
	float:left;
	margin-left:10%;
}
.contentdiv select{
	width:60%;
	height:35px;
	float:left;
	border:1px solid #ccc !important;
	padding:0;
}
.contentdiv input{
	width:60% !important;
	height:35px !important;
	float:left;
	border:1px solid #ccc !important;
	padding:0 !important;
}
.submitButton{
	width:50%;
	height:35px;
	background:#D13600;
	border-radius:3px;
	border:0;
	margin-left:25%;
	color:#fff;
}