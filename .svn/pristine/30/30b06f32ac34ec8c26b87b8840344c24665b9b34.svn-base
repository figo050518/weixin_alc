*{
	padding: 0;
	margin: 0;
}
.clear{
	clear: both;
}
body{
	font-family: helvetica,"microsoft yahei";
	color: #333;
	background: #FAFAFA;
	font-size: 62.5%;
}
.head{
	height:50px;
	background: #D13600;
	width: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 101;
}
.nickNameDiv{
	width: 100%;
	height: 50px;
	line-height: 50px;
	text-align: center;
	font-size: 16px;
	color: #fff;
}
.return{
	position: absolute;
	height: 40px;
	top: 6px;
	line-height: 40px;
	font-size: 16px;
	color: #fff;
}
.returnImg{
	position: relative;
    top: 5px;
    display: inline-block;
    margin-left: 3px;
    width: 24px;
    height: 24px;
    background-repeat: no-repeat;
    background-image: url(/resources/images/returnWhite.png);
    background-size: 24px 24px;
}
.goodsImg{
	width: 100vw;
	max-height: 100vw;
	display: block;
}
.goodsInfoDiv{
	border-bottom: 1px solid #EBECEE;
	background: #FFFFFF;
}
.goodsInfo1{
	width: 95vw;
	margin-left:5vw;
    border-bottom: 1px solid #EBECEE;
}
.goodsName{
	font-size: 14px;
	line-height: 8vw;
}
.goodsPrice{
	font-size: 14px;
	color: #D13600;
	line-height: 22px;
}
.goodsInfo2{
	width:95;
	color:#999999;
	font-size: 12px;
	width: 95vw;
	margin-left: 5vw;
	line-height: 10vw;
	height: 10vw;
}
.freight{
	float: left;
	width: 50%;
}
.inventory{
	float: left;
	width: 50%;
}
.goodsSizeDiv{
	margin-top: 10px;
}
.goodsSizeDiv{
	width: 100%;
	margin-top: 10px;
	background: #FFFFFF;
	border-bottom: 1px solid #EBECEE;
	border-top: 1px solid #EBECEE;
}
.goodsSize{
	width: 95vw;
	margin-left: 5vw;
	height: 10vw;
	line-height: 10vw;
	font-size: 14px;
}
.sellerInfo{
	width: 100%;
	margin-top: 10px;
	background: #FFFFFF;
	border-bottom: 1px solid #EBECEE;
	border-top: 1px solid #EBECEE;
}
.nickImg{
	position: relative;
    top: 1vw;
    display: inline-block;
    width: 5vw;
    height: 5vw;
    background-repeat: no-repeat;
    background-image: url(/resources/images/weixianshezhi.png);
    background-size: 5vw 5vw;
    margin-right: 2vw;
}
.guarantee{
	width: 95vw;
	margin-left: 5vw;
	height: 10vw;
	line-height: 10vw;
	font-size: 12px;
	color: #666666;
}
.guaranteeImg{
	position: relative;
    top: 0vw;
    display: inline-block;
    width: 4vw;
    height: 4vw;
    background-repeat: no-repeat;
    background-image: url(/resources/images/weixianshezhi.png);
    background-size: 4vw 4vw;
    margin-right: 2vw;
}
.shortDescriptionDiv{
	width: 100%;
	padding-top: 5vw;
}
.shortDescription{
	width: 95vw;
	margin-left: 5vw;
	font-size: 16px;
	line-height: 30px;
}

.support{
	width: 100%;
	text-align: center;
	padding: 5vw 0;
}
.supportImg{
	width: 20vw;
	height: 20vw;
}
.floor{
	position: fixed;
	width: 100%;
	left: 0px;
	bottom: 0px;
	background: #fff;
	border-top: 1px solid #EBECEE;
}
.floorTab{
	width: 94%;
	margin-left: 3%;
	height: 40px;
	margin-top: 5px;
}
.floorBtn{
	width: 100%;
	height: 40px;
	background:#D13600;
	outline: none;
	border: none;
	font-size: 14px;
	border-radius: 3px;
	color: #FFFFFF;
}
.shoppCartNum{
	position:absolute;
	background:#D13600;
	border-radius:50%;
	top:2px;
	right:8px;
	font-size:7px;
	padding:1px;
	width:14px;
	height:14px;
	text-align: center;
	line-height:14px;
	color: #FFFFFF;
}




.popupCover{
	position:fixed;
	_position:absolute;
	width:100%;
	height:100%;
	left:0;
	top:0; 
    background: #333333;
    opacity: 0.5;
    z-index:9990; 
    display: none;
}
.popup{
   position: fixed;
   background: #FFFFFF;
   z-index:9999;
   width: 100%;
   left:0;
   bottom: 0;
   display: none;
}
.goodsInfo{
	position:absolute;
	top:50%;
	left:65%;
	-webkit-transform: translate(-50%, -50%);
	transform: translate(-50%, -50%);
	float: left;font-size:1.4em;
	margin-left:10px;
}


.countDiv{
	float: right;
	font-size: 1.6em;
	height: 26px;
	margin-top: 12px;
	margin-right:3%;
}
.min{
	width: 26px;height: 26px;line-height: 26px;
	background: #D13600;text-align: center;
	float: left;
	font-size: 1.3em;
	color: #FFFFFF;
}
.count{
	float: left;
}
.add{
	width: 26px;height: 26px;line-height: 26px;
	background: #D13600;text-align: center;
	float: left;
	font-size: 1.3em;
	color: #FFFFFF;
}
.addShoppingCart{
	width:50%;float: left;
	background:#FF9100;height:50px;font-size:1.6em;
	line-height: 50px;
	text-align: center;
	color:#fff;
	border:none;
	outline:none;
}
.buyNowGoods{
	float: left;
	width: 50%;
	background:#D13600;
	text-align: center;
	height: 50px;
	line-height:50px;
	font-size:1.6em;
	color: #FFFFFF;
	border:none;
	outline:none;
}
.sureBtn{
	width: 100%;
	background:#F7E05F;
	text-align: center;
	height: 50px;
	line-height:50px;
	font-size:1.6em;
}
.mjPopup{
	position: fixed;
   background: #FFFFFF;
   z-index:9999;
   width: 100%;
   left:0;
   bottom: 0;
   display: none;
}
.overHiden{overflow: hidden;height: 100%;}

.instructionsPopup{
	position: fixed;
   background: #FFFFFF;
   z-index:9999;
   width: 100%;
   left:0;
   bottom: 0;
   display: none;
}
.instructionsTab{
	background:#F7E05F;
	height:40px;
	width:100%;
}
.operationGoods{
	width: 90%;
    height: 35px;
    background: #fff;
    outline: none;
    border: none;
    font-size: 16px;
    border-radius: 3px;
    border: 1px solid #EBECEE;
    box-sizing: border-box;
    font-size: 14px;
}
.promote{
	background: #D13600;
	border: 1px solid #D13600;
	color: #FFFFFF;
}
.navigationDiv{
	position: absolute;
	left: calc(18vw - 43px);
	bottom: 55px;
}
.navigation{
	background: #000000;
	opacity: 0.8;
	padding: 0 10px;
	border-radius: 6px;
	font-size: 14px;
	width: 66px;
}
#test1 {
 	width:0;
    height:0;           
    border-width:7px 7px 0px 7px;           
    border-style:solid;            
    border-color: #000000 transparent transparent transparent;
    opacity: 0.7;
    margin-left: 38px;
    margin-top: -1px;
}
.toOtherPageDiv{
	height: 40px;
    position: absolute;
    top: 5px;
    right: 2vw;
    line-height: 40px;
    font-size: 16px;
    color: #FFFFFF;
    padding: 0 2vw;
    font-size: 0;
}
.toOthwePage{
	width: 22px;
	height: 22px;
	margin-top: 9px;
}

.tableDiv{
	background: #000000;
	opacity: 0.7;
	z-index: 10;
	padding: 0 10px;
	border-radius: 6px;
}
#sanjiao {
 	width:0;
    height:0;           
    border-width:0px 7px 10px 7px;           
    border-style:solid;            
    border-color:transparent transparent #000000 transparent;
    opacity: 0.7;
    margin-left: 80px;
}
.headNav{
	position: fixed;
	top:40px;
	right: 3vw;
	z-index: 105;
}
.detailsDiv{
	background: #FFFFFF;
}
.detailsImgList{
	width: 100%;
}
.detailsImgList{
	font-size: 0px;
}
.colorType {
    font-size: 1.2em;
    border: 1px solid #C9C9C9;
    margin-top: 10px;
    height: 28px;
    line-height: 28px;
    padding: 0px 8px;
    display: inline-block;
    text-align: center;
    overflow: hidden;
    vertical-align: middle;
    border-radius: 10px;
    margin: 10px 10px 0 0;
    font-style: normal;
    background: #F5F5F5;
    color: #333;
}