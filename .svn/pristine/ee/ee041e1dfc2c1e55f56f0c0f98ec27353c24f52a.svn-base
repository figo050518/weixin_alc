*{
	padding: 0;
	margin: 0;
}
.clear{
	clear: both;
}
a{
	color:#333;
}
body{
	width:100%;
	height: 100%;
	font-size:62.5%;
	background:#F5F5F5;
}
.head{
	height:50px;
	background: #D13600;
	width: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 101;
}
.title{
	line-height: 50px;
	font-size: 16px;
	color: #FFFFFF;
	text-align: center;
}
.return{
	position: absolute;
	height: 40px;
	top: 6px;
	line-height: 40px;
	font-size: 16px;
	color: #fff;
}
.returnImg{
	position: relative;
    top: 5px;
    display: inline-block;
    margin-left: 3px;
    width: 24px;
    height: 24px;
    background-repeat: no-repeat;
    background-image: url(/resources/images/returnWhite.png);
    background-size: 24px 24px;
}
.wuliuContent{
	width:100%;
	height:100px;
	position:relative;
	background: #fff;
	margin-top:50px;
	padding:15px 0;
}
.ContentItem{
	width:60%;
	height:35px;
	line-height:35px;
	position:relative;
	text-align: left;
	margin-left:5%;
}
.wuliuStatus{
	color:#D13600;
	font-size: 14px;
	margin-left: 10px;
}
.wuliuCpy{
	color:#999;
	font-size: 14px;
	margin-left: 10px;
}
.wuliuNo{
	color:#999;
	font-size: 14px;
	margin-left: 10px;
}
.wuliuList{
	width:100%;
	background: #fff;
	position: relative;
	padding: 10px 0;
}
.ulInfo{
	width:88vw;
	position:relative;
	margin: 0;
	padding: 0;
	margin-left: 6vw;
	list-style: none;
	border-left: 1px solid #ddd;
}
.ulInfo li{
	width: 100%;
	line-height: 25px;
	position: relative;
	padding: 0;
}
.ulInfo li::before{
	content: '';
    border: 3px solid #f3f3f3;
    background-color: #d9d9d9;
    display: inline-block;
    width: 5px;
    height: 5px;
    border-radius: 5px;
    margin-left: -6px;
    margin-right: 10px;
}
.ulInfo li:nth-child(1)::before{
	content: '';
    border: 3px solid #EA9D9D;
    background-color: #D13600;
    display: inline-block;
    width: 5px;
    height: 5px;
    border-radius: 5px;
    margin-left: -6px;
    margin-right: 10px;
}
.ulInfo li:nth-child(1) .conDiv{
	color:#D13600
}
.conDiv{
	width:90%;
	position: relative;
	float: right;
	font-size: 12px;
	padding-bottom: 10px;
	border-bottom:1px solid #ddd;
}
.conDiv span{
	display: block;
}
#time{
	height:20px;
}

.infoNone{
	width:100%;
	height: 100px;
	line-height: 100px;
	text-align: center;
	font-size: 14px;
}
