*{
	padding: 0;
	margin: 0;
}
.clear{
	clear: both;
}
body{
	font-family: helvetica,"microsoft yahei";
	color: #333;
	background: #FAFAFA;
	font-size: 62.5%;
}
.head{
	height:50px;
	background: #D13600;
	width: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 101;
}

.searchInputType{
	width:70vw !important;
	position:absolute;
    top:50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
	height: 34px !important;
	border:2px solid #D13600;
	border-radius:8px;
	padding: 0 !important;
	padding-left: 30px !important;
	outline: none;
	font-size: 14px !important;
	background: url(/resources/images/icon_search.png) no-repeat scroll right center transparent;
    background-size: 20px 20px;
	outline: none;
	background-position:5px 5px;
	background-color:#fff !important;
	text-align: left !important; 
}
.searchInputType::-webkit-input-placeholder { /* WebKit browsers */ 
	
} 
.return{
	position: absolute;
	height: 40px;
	top: 6px;
	line-height: 40px;
	font-size: 16px;
	color: #fff;
}
.returnImg{
	position: relative;
    top: 5px;
    display: inline-block;
    margin-left: 3px;
    width: 24px;
    height: 24px;
    background-repeat: no-repeat;
    background-image: url(/resources/images/returnWhite.png);
    background-size: 24px 24px;
}
.headLeft{
	width: 20vw;
	border-right: 1px solid #F5F5F5;
	height: 40px;
	line-height: 40px;
	color: #D13600;
	box-sizing: border-box;
	text-align: center;
	font-size: 14px;
	background: #FFFFFF;
	color: #D13600;
}
#sliderSegmentedControl{
	position: absolute;
	left:20vw;
	margin: 0 !important;
	color: #D13600;
}
#pinleiList .mui-active{
	background: #D13600;
	color: #FFFFFF;
}
.oneGoods{
	width: 100%;
	position: relative;
	padding: 3vw 0;
	border-bottom: 1px solid #EBECEE;
}
.goodsImgDiv{
	width: 22vw;
	margin-left: 3vw;
	float: left;
	height: 22vw;
}
.goodsImg{
	width: 22vw;
	height: 22vw;
	display: block;
	border-radius: 3px;
}
.goodsInfo{
	margin-left: 3vw;
	width: 57vw;
	height: 22vw;
	float: left;
	position: relative;
	border-right: 1px solid #EBECEE;
}
.goodsName{
	margin-top: 1vw;
	width: 100%;
	-webkit-line-clamp: 2;
	text-overflow: ellipsis;
	overflow: hidden;
	-webkit-box-orient: vertical;
	display: -webkit-box;
	font-size: 4vw;
}
.goodsInfoFloor{
	position: absolute;
	left: 0;
	bottom:1vw;
	font-size: 3.5vw;
}
.price{
	color: #D13600;
}
.managAndShare{
	float: left;
	height: 22vw;
	width:calc(15vw - 1px);
	position: relative;
}
.managementImg{
	width: 8vw;
	height: 8vw;
	display: block;
}
.managementText{
	width: 100%;
	text-align: center;
	color: #999999;
	margin-top: 1vw;
	font-size: 12px !important;
}

.managementDiv{
	position:absolute;
    top:50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
}
.pinleiDiv{
	border-bottom: 1px solid #EBECEE;
}


.overlay{
	position:fixed;
	_position:absolute;
	width:100%;
	height:100%;
	left:0;
	top:0; 
	/*display: none;*/
    background-color:rgba(0,0,0,0.7);
    z-index:9990; 
}
.popup{
	display: none;
   position: fixed;
   _position: absolute;
   width: 80%;
   background: #FFFFFF;
   outline: none;
   border-radius: 3px;
   background-color: white; 
   z-index:9999; 
   top:50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  padding: 5vw 0;
}


.changePriceDiv{
	margin-left: 5vw;
}
.changeText{
	font-size: 14px;
}
.changeInput{
	padding:0 !important;
	margin:0 !important;
	float:left;
	margin-right: 3vw !important;;
	width:40vw !important;
	font-size:14px !important;;
	height: 9vw !important;;
	box-sizing: border-box;
	border: 1px solid #D13600;
	text-align: center;
}
.changeInput::-webkit-input-placeholder{
	text-align: center;
}
.priceIcon{
	float: left;
	line-height: 9vw;
	font-size: 3.4vw;
	margin-right:5px;
}
.changeBtn{
	padding:0 !important;
	margin:0 !important;
	width: 70vw;
	height: 10vw;
	background: #D13600 !important;
	border: none;
	outline: none;
	font-size: 14px !important;
	color: #FFFFFF !important;
	margin-left:5vw !important;
	margin-top:5vw !important;
}
.closeImgDiv{
	position:absolute;
	width:30px;
	height:30px;
	font-size:0;
	top:5px;
	right:5px;
}
.closePopup{
	width:20px;
	height:20px;
	margin:5px;
}