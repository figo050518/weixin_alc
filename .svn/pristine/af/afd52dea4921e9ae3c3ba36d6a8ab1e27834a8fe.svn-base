.zhezhao{
	width:100%;
	height:100%;
	position:fixed;
	background:rgba(0,0,0,0.5);
	z-index:999;
	top:0;
	left:0;
}
.trackingNo{
	width:100%;
	height:100%;
	position:fixed;
	top:0;
	left:0;
	z-index:1000;
	background:#fff;
	font-size:14px;
}
.trackingNo .title{
	width:100%;
	background:#D13600;
	color:#fff;
	height:50px;
	line-height:50px;
	text-align:center;
	font-size:16px;
}
.trackingNo .content{
	width:100%;
	height:170px;
	position:relative;
	padding:15px 0;
}
.trackingNo .contentdiv{
	width:100%;
	height:35px;
	line-height:35px;
	position:relative;
}
.contentdiv div{
	width:20%;
	height:35px;
	float:left;
	margin-left:10%;
}
.contentdiv select{
	width:60%;
	height:35px;
	float:left;
	border:1px solid #ccc !important;
	padding:0;
}
.contentdiv input{
	width:60% !important;
	height:35px !important;
	float:left;
	border:1px solid #ccc !important;
	padding:0 !important;
}
.submitButton{
	width:50%;
	height:35px;
	background:#D13600;
	border-radius:3px;
	border:0;
	margin-left:25%;
	color:#fff;
}

