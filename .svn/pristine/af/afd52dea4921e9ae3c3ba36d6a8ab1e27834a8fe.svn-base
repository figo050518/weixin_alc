*{
	padding: 0;
	margin: 0;
}
.clear{
	clear: both;
}
body{
	font-family: helvetica,"microsoft yahei";
	color: #333;
	background: #FAFAFA;
}
.head{
	height:50px;
	background: #D13600;
	width: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 101;
}
.headNav{
	width: 50%;
	height: 30px;
	box-sizing: border-box;
	border: 1px solid #FFFFFF;
	border-radius: 4px;
	position:absolute;
    top:50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    line-height: 30px;
    font-size: 14px;
}
.headNavCover{
	width: 100%;
	height: 100%;
	border-radius: 4px;
	padding: 1px;
	position:absolute;
    top:-1px;
    left: -1px;
    background: #D13600;
    opacity: 0.5;
    display: none;
}
.headNavL{
	width: 50%;
	height: 100%;
	background: #D13600;
	float: left;
	text-align: center;
	border-radius:3px 0 0 3px !important;
	color: #FFFFFF;
}
.headNavR{
	width: 50%;
	height: 100%;
	float: left;
	background: #FFFFFF;
	border-radius: 0 3px 3px 0 !important;
	text-align: center;
	color: #D13600;
}
.return{
	position: absolute;
	height: 40px;
	top: 6px;
	line-height: 40px;
	font-size: 16px;
	color: #fff;
}
.returnImg{
	position: relative;
    top: 5px;
    display: inline-block;
    margin-left: 3px;
    width: 24px;
    height: 24px;
    background-repeat: no-repeat;
    background-image: url(/resources/images/returnWhite.png);
    background-size: 24px 24px;
}
.tapDiv{
	width: 100%;
	height: 10vw;
	line-height: 10vw;
	font-size: 3vw;
	text-align: center;
	position: fixed;
	top: 50px;
	left: 0;
	box-sizing: border-box;
	z-index: 101;
	background:#FAFAFA;
}
.tap{
	width: 100%;
	height: 100%;
	box-sizing: border-box;
}
#tap1{
	
}
.goodsList{
	border-bottom: 1px solid #EBECEE;
}
.oneGoods{
	width: 100%;
	position: relative;
}
.goodsImgDiv{
	width: 22vw;
	margin-left: 3vw;
	float: left;
	height: 28vw;
}
.goodsImg{
	width: 22vw;
	height: 22vw;
	display: block;
	border-radius: 3px;
	margin-top: 3vw;
}
.goodsInfo{
	margin-left: 3vw;
	width: 72vw;
	height: 28vw;
	float: left;
	position: relative;
	border-bottom: 1px solid #EBECEE;
}
.goodsName{
	margin-top: 4vw;
	width: 100%;
	-webkit-line-clamp: 2;
	text-overflow: ellipsis;
	overflow: hidden;
	-webkit-box-orient: vertical;
	display: -webkit-box;
	font-size: 4vw;
}
.goodsInfoFloor{
	position: absolute;
	left: 0;
	bottom:4vw;
	font-size: 3.5vw;
}
.price{
	color: #D13600;
}
.blank{
	width: 100%;
	height:calc(52px + 10vw);
}
.orderInfoDiv{
	width: 95vw;
	margin-left: 5vw;
	color: #999999;
	font-size: 3.5vw;
	height: 8vw;
	line-height: 8vw;
	border-bottom: 1px solid #EBECEE;;
}
.orderNum{
	float: left;
}
.orderTime{
	float: right;
	margin-right: 5vw;
}
.orderState{
	float: right;
	margin-right: 5vw;
	color: #FBBE88;
	font-size: 4vw;
}
.blankState{
	width: 100vw;
	height: 100vh;
	position: absolute;
	top: 0;
	left: 0;
	background: #FAFAFA;
	z-index: 100;
}
.blankImgDiv{
	width: 80vw;
	margin-left: 10vw;
	margin-top: 40vw;
}
.blankImg{
	width: 100%;
	display: block;
}
.blankText{
	width: 100%;
	text-align: center;
	font-size: 16px;
	margin-top: 5vw;
	color: #B0B9C2;
}
.blankBtnDiv{
	width: 100%;
	height: 40px;
	position: relative;
	margin-top: 8vw;
	text-align: center;
}
.blankBtn{
	outline: none;
	border: none;
	font-size: 16px;
	border-radius: 20px;
	padding: 0 30px;
	color: #FFFFFF;
	background: #D13600;
	height: 40px;
}
.oneOrder{
	border-bottom: 1px solid #EBECEE;
	margin-top: 2px;
}
.goodsOperation{
	height: 12vw;
}
.goodsStatus{
	float: left;
	margin-left: 3vw;
	font-size: 14px;
	color: #FBBE88;
	border: 1px solid #FBBE88;
	box-sizing: border-box;
	height: 8vw;
	line-height: 8vw;
	margin-top: 2vw;
	padding: 0 10px;
	border-radius: 5px;
}
.goodsBtn{
	float: right;
	margin-right: 3vw;
	font-size: 14px;
	color: #FFFFFF;
	border: 1px solid #D13600;
	box-sizing: border-box;
	height: 8vw;
	margin-top: 2vw;
	padding: 0 10px;
	border-radius: 5px;
	background: #D13600;
}




.popupOverlay{
	position:fixed;
	width:100%;
	height:100%;
	left:0;
	top:0; 
	display: none;
    background:#000;
    z-index:9990;
    opacity:0.7;
    filter:progid:DXImageTransform.Microsoft.Alpha(Opacity=70);
}
.promptPopup{
	display: none;
    position: fixed;
    width:400px;
    background: #FFFFFF;
    outline: none;
    background-color: white; 
    z-index:99999; 
    top:50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    padding-bottom: 30px;
    /* border-radius:10px 10px 0 0; */
}
.popupSure{
	background: #FFFFFF;
	width: 35%;
	height:30px;
	outline: none;
	border: none;
	font-size: 16px;
	margin-top: 40px;
	background: #FFD900;
	border-radius:3px;
	font-family: "Microsoft YaHei";
	border:1px solid #FFD900;
}
.popupCencle{
	background: #FFFFFF;
	width: 35%;
	height:30px;
	outline: none;
	border: none;
	font-size: 16px;
	margin-top: 40px;
	background: #fff;
	border-radius:3px;
	font-family: "Microsoft YaHei";
	border:1px solid #DCDCDC;
}
.promptText{
	height:40px;
	/* background:#FFD900; */
	line-height:40px;
	width:100%;
	font-size:14px;
	border-bottom:1px dashed #A9A9A9;
	color:#A9A9A9;
	/* border-radius:10px 10px 0 0; */
}
.closeDiv{
	position:absolute;
	height:20px;top:10px;
	right:10px;width:20px;
	cursor:pointer;
}
.closeImg{
	height:12px;
	margin-top:4px;
	margin-right:14px;
}
.popupMsgDiv{
	text-align: center;
	font-size:14px;
	font-family: 'Microsoft YaHei';
	line-height: 30px;margin-top:40px;
}
.css3-container { filter:alpha(opacity=50); }
.productBody:hover{
