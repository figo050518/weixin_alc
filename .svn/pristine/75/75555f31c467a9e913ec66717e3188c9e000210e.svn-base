.overlay{
	width: 100%;
	height: 100%;
	background: rgba(0,0,0,0.5);
	left: 0;
	top: 0;
	z-index: 5;
	position: fixed;
	display: none;
}
.radio{
	-webkit-appearance: none;
    position: relative;
    width: 20px;
    height: 20px;
    -webkit-transition: all 0.15s ease-out 0;
    background: #fff;
    border: none;
    color: #fff;
    display: inline-block;
    outline: none;
    position: relative;
    margin-right: 0.5rem;
    z-index: 1;
    border: 1px solid #ccc;
    border-radius: 50%;
}
.radio:checked{
	background: #D13600;
	border:1px solid #D13600;
}
.radio:checked:before{
	opacity: 1;
    content: '';
    position: absolute;
    width: 8px;
    height: 5px;
    background: transparent;
    top: 4px;
    left: 4px;
    border: 2px solid #fff;
    border-top: none;
    border-right: none;
    -webkit-transform: rotate(-45deg);
}