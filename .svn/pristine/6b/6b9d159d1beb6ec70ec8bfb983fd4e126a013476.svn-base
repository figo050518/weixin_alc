body {
	padding: 0;
	margin: 0;
	background: #fff;
}

.header {
	width: 100%;
	position: relative;
	border-bottom:1px solid #eee;
}

.headerbanner {
	width: 100%;
	height: 150px;
	background: url(/resources/images/0052_foodsense.jpg) no-repeat center center;
	background-size: 100% 100%;
	position: relative;
}

.shopLogo {
	position: relative;
	width: 80px;
	height: 80px;
	background: #fff;
	left: 50%;
	margin-left: -40px;
	margin-top: -50px;
}

.shopLogo img {
	width: 76px;
	height: 76px;
	position: absolute;
	margin-top: 2px;
	margin-left: 2px;
}

.shopname {
	width: 100%;
	text-align: center;
	height: 25px;
	position: relative;
	line-height: 25px;
	font-size: 14px;
	color: #333;
}

.shopInfo {
	width: 90%;
	height:20px;
	text-align: center;
	position: relative;
	line-height: 20px;
	font-size: 12px;
	margin-left: 5%;
	color: #999;
	overflow:hidden;
	margin-bottom:10px;
}

.classify {
	width: 100%;
	position: relative;
}

.classifyone {
	width: 100%;
	position: relative;
	display: -webkit-flex;
	/* Safari */
	display: flex;
	flex-direction: row;
    flex-wrap: wrap;
}

.itemClass {
	width:25vw;
	text-align: center;
	height: 80px;
	position: relative;
}

#imgDiv {
	width: 50px;
	height: 50px;
	background: #f5f5f5;
	border-radius: 50%;
	position: relative;
	left: 50%;
	margin-left: -25px;
}

#imgDiv img {
	width: 30px;
	height: 30px;
	margin-top:10px;
}

.itemClass p {
	font-size: 14px;
	color: #666;
}

.recommended {
	width: 100%;
	position:relative;
}
.recommendedTitle{
	width: 100%;
	height: 40px;
	text-align: center;
	font-size: 14px;
	color: #333;
	line-height:40px;
}
.recommendedTitle div:nth-child(1){
	width:25px;
	height:25px;
	margin-top:7px;
	border-radius:12.5px;
	overflow:hidden;
	position:absolute;
	left:50%;
	margin-left:-65px;
	background:rgba(255,0,0,0.3);
}
.recommendedTitle div:nth-child(1) img{
	width:16px;
	height:16px;
	position:absolute;
	top:4.5px;
	left:4.5px;
}
.Titletext{
	width:60px;
	line-height:30px;
	font-size:14px;
	margin-top:-10px;
	display:inline-block;
	
}
.recommendedCon {
	width: 100%;
	position: relative;
}

.recommendedOne {
	width: 45vw;
	border-bottom: 1px solid #eee;
	margin: 2.5vw;
	padding: 2.5vw 0;
	float: left;
	position: relative;
	text-align:center;
}

.recommendedOne img {
	width: 30vw;
	height: 30vw;
	position: relative;
}

.fahuo {
	width: 100%;
	text-align: left;
	line-height:6vw;
	position: relative;
	font-size: 3.5vw;
	color: #999;
	margin-top:2vw;
}

.pdtName {
	width: 100%;
	height: 12vw;
	line-height: 6vw;
	font-size: 4vw;
	text-align: left;
}

.pdtPrice {
	text-align: left;
	color: #E62234;
	height: 6vw;
	line-height: 6vw;
	margin-top:10px;
}

.mui-table-view-cell:after {
	height: 0px;
}

.mui-table-view:after {
	height: 0px;
}

.mui-pull-bottom-pocket {
	height: 50px;
}

#pullrefresh {
	background: #fff;
	top:250px !important;
	margin-bottom: 50px;
	background: #fff;
}