package com.fcgo.weixin.web.view.entity;

public class DataGroup {

    public DataGroup(String text, String value, boolean disable) {
        this.text = text;
        this.value = value;
        this.disable = disable;
    }

    public DataGroup(String text, String value) {
        this.text = text;
        this.value = value;
    }

    public DataGroup() {

    }

    private String text;
    private String value;
    private boolean disable;

    public String getText() {
        return text;
    }

    public void setText(String text) {
        this.text = text;
    }

    public String getValue() {
        return value;
    }

    public void setValue(String value) {
        this.value = value;
    }

    public boolean isDisable() {
        return disable;
    }

    public void setDisable(boolean disable) {
        this.disable = disable;
    }
}
