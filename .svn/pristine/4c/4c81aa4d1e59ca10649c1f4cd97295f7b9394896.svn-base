*{
	padding: 0;
	margin: 0;
}
.clear{
	clear: both;
}
body{
	font-family: helvetica,"microsoft yahei";
	color: #333;
	background: #FAFAFA;
	font-size: 62.5%;
}
.head{
	height:50px;
	background: #D13600;
	width: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 101;
}
.title{
	line-height: 50px;
	font-size: 16px;
	color: #FFFFFF;
	text-align: center;
}
.releaseDiv{
	height: 40px;
	position: absolute;
	top: 5px;
	right: 3vw;
	line-height: 40px;
	font-size: 16px;
	color: #FFFFFF;
}

.oneArea{
	background: #FFFFFF;
	width: 100%;
	margin-top: 10px;
	border-bottom: 1px solid #EBECEE;
	border-top: 1px solid #EBECEE;
}
.oneArea .couponsDiv:last-child{
	border-bottom:0px solid #F1F1F1;
}
.couponsDiv{
	width: 95vw;
	margin-left: 5vw;
	border-bottom: 1px solid #EBECEE;
	position: relative;
}
.couponsDiv a{
	color:#333;
}
.leftImgDiv{
	float:left;
}
.leftImg{
	height: 36px;
	margin-top: calc(7.5vw - 18px);
}
.leftText{
	float:left;
	font-size: 1.6em;
	margin-left: 20px;
	line-height: 15vw;
}
.toNextDiv{
	float:right;
	margin-right: 5vw;
}
.toNext{
	height: 16px;
	margin-top: calc(7.5vw - 8px);
}
.mobilePhone{
	font-size: 16px;
	position: absolute;
	line-height: 15vw;
	top: 0;
	right: calc(5vw + 20px);
}
