body{
	padding: 0;
	margin: 0;
	position: relative;
}
.head{
	height:50px;
	background: #D13600;
	width: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 101;
}
.title{
	line-height: 50px;
	font-size: 16px;
	color: #FFFFFF;
	text-align: center;
}
.return{
	position: absolute;
	height: 40px;
	top: 6px;
	line-height: 40px;
	font-size: 16px;
	color: #fff;
}
.returnImg{
	position: relative;
    top: 5px;
    display: inline-block;
    margin-left: 3px;
    width: 24px;
    height: 24px;
    background-repeat: no-repeat;
    background-image: url(/resources/images/returnWhite.png);
    background-size: 24px 24px;
}

.content{
	width:100%;
	height:100%;
	background: #fff;
	position: relative;
	margin-top:60px;
}
.contentTop{
	width:100%;
	height:120px;
	position: relative;
	padding: 15px 0;
}
.contentTop img{
	width:28%;
	position: relative;
	margin-left:5%;
	height:120px;
	float: left;
	
}
.contentTop .text{
	width: 30%;
	height: 120px;
	position: relative;
	text-align: center;
	float: left;
	color:#D13600;
	font-size: 18px;
	line-height: 60px;
	margin-left: 5%;
}
.contentBottom{
	width: 100%;
	height: 100%;
	position: relative;
	background: #fff;
	padding: 10px 0;
}
.contentBottom p{
	width:90%;
	margin-left: 5%;
	color:#333;
	font-size: 14px;
}
.contentBottom p span:nth-child(1){
	color:#666;
	float: left;
	width: 25%;
}
.contentBottom p span:nth-child(2){
	color:#666;
	float: left;
	width: 75%;
}
.contentBottom p span{
	display: block;
	line-height: 25px;
}
 .text button{
	width:80px;
	height: 23px;
	margin-left: 5%;
	background: #fff;
	border:1px solid #D13600;
	color:#D13600;
	border-radius: 3px;
	font-size: 14px;
	outline:none;
}
