body{
	background: #FAFAFA;padding: 0;margin: 0;
	color: #333;
	font-family: helvetica,"microsoft yahei";

}
.titleTable{
	height: 50px;
	width: 100%;
}
.titleTable td{
	background:#D13600;
	padding:0 2vw;
	color: #fff;
	font-size: 16px;
}
ul{
	padding: 0;
	margin: 0;
	
}
a{
	color: blue;
	text-decoration: none;
	font-size: 3.3vw;
}
p{
	padding: 0;
	margin: 1vw 0;
}
.content{
	position: relative;
	width: 100%;
	
}
.content table{
	border-bottom: 1px solid #EBECEE;
	width: 100%;
}
.td{
	background: #fff;
	color:#333;
	font-size: 14px;
	padding: 4vw;
}
.SZtable th{
	font-size: 14px;
	text-align: center;
	font-weight: 400;
	padding: 3vw 0;
}
.SZtable td{
	background: #fff;
	color:#333;
	font-size: 13px;
	text-align: center;
	padding: 3vw 0;
	border-bottom: 1px solid #EBECEE;
}
.tableQH{
	width:100%;
	height: 40px;

}
.tableQH td{
	background: #f5f5f5;
	text-align: center;
}
.tableQH td:nth-child(2n-1){
	border:0 !important;
}
.tableQH td:nth-child(2n-1).on{
	border:0 !important;
}
.tableQH td.on{
	border-bottom:2px solid #D13600;
}