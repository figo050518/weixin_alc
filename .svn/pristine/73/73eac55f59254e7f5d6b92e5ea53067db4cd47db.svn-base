body{
	background: #FAFAFA;padding: 0;margin: 0;
	color: #333;
	font-family: helvetica,"microsoft yahei";
}
.clear{
	clear: both;
}
.head{
	height:50px;
	background: #D13600;
	width: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 101;
}
.title{
	line-height: 50px;
	font-size: 16px;
	color: #FFFFFF;
	text-align: center;
}
.return{
	position: absolute;
	height: 40px;
	top: 6px;
	line-height: 40px;
	font-size: 16px;
	color: #fff;
}
.returnImg{
	position: relative;
    top: 5px;
    display: inline-block;
    margin-left: 3px;
    width: 24px;
    height: 24px;
    background-repeat: no-repeat;
    background-image: url(/resources/images/returnWhite.png);
    background-size: 24px 24px;
}
.TXtable{
	margin-top:50px;
	background:#ffffff;
	height:10vw;
	line-height:10vw;
	font-size:14px;
	text-align: center;
}
.oneDrawApply{
	width:100vw;
	padding: 3vw 0;
}
.time{
    background: #fff;
    color: #333;
    font-size: 14px;
    text-align: center;
    width:50vw;
    text-align: center;
    float:left;
}
.price{
    background: #fff;
    color: #333;
    font-size: 14px;
    text-align: center;
    width:25vw;
    text-align: center;
    float:left;
}
.status{
    background: #fff;
    color: #333;
    font-size: 14px;
    text-align: center;
    width:25vw;
    text-align: center;
    float:left;
}
.blankState{
	width: 100vw;
	height: 100vh;
	position: absolute;
	top: 0;
	left: 0;
	background: #FAFAFA;
	z-index: 100;
}
.blankImgDiv{
	width: 80vw;
	margin-left: 10vw;
	margin-top: 40vw;
}
.blankImg{
	width: 100%;
	display: block;
}
.blankText{
	width: 100%;
	text-align: center;
	font-size: 16px;
	margin-top: 5vw;
	color: #B0B9C2;
}
.blankBtnDiv{
	width: 100%;
	height: 40px;
	position: relative;
	margin-top: 8vw;
	text-align: center;
}
.blankBtn{
	outline: none;
	border: none;
	font-size: 16px;
	border-radius: 20px;
	padding: 0 30px;
	color: #FFFFFF;
	background: #D13600;
	height: 40px;
}