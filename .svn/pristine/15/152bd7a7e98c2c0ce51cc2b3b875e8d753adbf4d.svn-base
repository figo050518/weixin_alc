<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
 PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
 "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <!-- 定义加载的数据库驱动 -->
    <classPathEntry location="C:/Users/xiahanxzh/.m2/repository/mysql/mysql-connector-java/5.1.25/mysql-connector-java-5.1.25.jar"/>

    <!-- 目标只能是MyBatis3 -->
    <context id="mysqlShowTabels" targetRuntime="Mybatis3">
    
    	<plugin type="com.fcgo.weixin.persist.mybatis.generator.plugin.CustomizeContextPlugin">
            <property name="poSuffix" value="PO"/>
            <property name="daoSuffix" value="DAO"/>
            <property name="dtoSuffix" value="DTO"/>
            <property name="customDtoPrjDir" value="C:/fcgo/weixin-parent/weixin-web-common/src/main/java/"/>
            <property name="customDaoPrjDir" value="C:/fcgo/weixin-parent/weixin-persist/src/main/java/"/>
            <property name="customDtoPkg" value="com/fcgo/weixin/web/dto"/>
            <property name="customDtoPkgName" value="com.fcgo.weixin.web.dto"/>
            <property name="customDaoPkg" value="com/fcgo/weixin/persist/dao"/>
            <property name="customDaoPkgName" value="com.fcgo.weixin.persist.dao"/>
        </plugin>

        <plugin type="com.fcgo.weixin.persist.mybatis.generator.plugin.ReplaceExamplePlugin">
            <property name="searchString" value="Example$"/>
            <property name="replaceString" value="Criteria"/>
        </plugin>

        <plugin type="com.fcgo.weixin.persist.mybatis.generator.plugin.CustomizePlugin"/>

		<plugin type="com.fcgo.weixin.persist.mybatis.generator.plugin.UpdateRecordWithoutPrimaryKeyPlugin"/>

        <commentGenerator type="com.fcgo.weixin.persist.mybatis.generator.plugin.DatabaseCommentGenerator">
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="false"/>
        </commentGenerator>

        <!-- 数据库的配置 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
         connectionURL="jdbc:mysql://112.74.190.37:3306/fcg_wd?userUnicode=true&amp;characterEncoding=UTF-8&amp;zeroDateTimeBehavior=convertToNull"
         userId="root" password="test123">
        </jdbcConnection>

        <!-- 是否强制转换 只有forceBigDecimals 一个属性 -->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <!-- 生成po -->
        <javaModelGenerator targetPackage="com.fcgo.weixin.persist.po" targetProject="src/main/java">
            <property name="enableSubPackages" value="false"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <!-- 用来定义返回类型的属性 mapper.xml -->
        <sqlMapGenerator targetPackage="context.mybatis.generate.sqlmap" targetProject="src/main/resources">
            <property name="enableSubPackages" value="false"/>
        </sqlMapGenerator>

        <!-- 生成Mapper接口 -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.fcgo.weixin.persist.generate" targetProject="src/main/java">
            <property name="enableSubPackages" value="false"/>
        </javaClientGenerator>

		
		<table schema="aizhu" tableName="Test" domainObjectName="UserInfo">
		</table>
		
    </context>
</generatorConfiguration>
