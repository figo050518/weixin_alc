<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fcgo.weixin.persist.dao.IUserInfoDAO" >
  <resultMap id="userInfoPagerResultMap" type="com.fcgo.weixin.persist.po.UserInfoPO" >
    <id column="ID" property="id" jdbcType="INTEGER" />
    <result column="IS_DELETE" property="isDelete" jdbcType="INTEGER" />
    <result column="CREATE_TIME" property="createTime" jdbcType="TIMESTAMP" />
    <result column="UPDATE_TIME" property="updateTime" jdbcType="TIMESTAMP" />
    <result column="WX_ID" property="wxId" jdbcType="VARCHAR" />
    <result column="TEL_NUM" property="telNum" jdbcType="VARCHAR" />
    <result column="NIKE_NAME" property="nikeName" jdbcType="VARCHAR" />
    <result column="create_name" property="createName" jdbcType="VARCHAR" />
    <result column="update_name" property="updateName" jdbcType="VARCHAR" />
    <result column="fcg_seller_id" property="fcgSellerId" jdbcType="INTEGER" />
  </resultMap>
    <select id="lisPagertUserInfo" parameterType="map" resultMap="userInfoPagerResultMap">
  
  		select IS_DELETE,CREATE_TIME,UPDATE_TIME,WX_ID,TEL_NUM,NIKE_NAME,create_name,update_name,fcg_seller_id  from user_info 
  		
  		 where user_type='2'
  		 
  		<if test="nikeName!=null and nikeName!=''">
			AND nike_Name like #{nikeName,jdbcType=VARCHAR} 
		</if>
  		 
  		 order by update_time desc
  		 limit #{startPage, jdbcType=DECIMAL},#{pageSize, jdbcType=DECIMAL} 
  		 
  
  </select>
</mapper>