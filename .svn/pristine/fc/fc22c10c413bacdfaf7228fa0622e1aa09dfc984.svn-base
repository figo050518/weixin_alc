*{
	padding: 0;
	margin: 0;
}
.clear{
	clear: both;
}
body{
	font-family: helvetica,"microsoft yahei";
	color: #333;
	background: #FAFAFA;
	font-size: 62.5%;
}
.head{
	height:50px;
	background: #D13600;
	width: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 101;
}
.title{
	line-height: 50px;
	font-size: 16px;
	color: #FFFFFF;
	text-align: center;
}
.return{
	position: absolute;
	height: 40px;
	top: 6px;
	line-height: 40px;
	font-size: 16px;
	color: #fff;
}
.returnImg{
	position: relative;
    top: 5px;
    display: inline-block;
    margin-left: 3px;
    width: 24px;
    height: 24px;
    background-repeat: no-repeat;
    background-image: url(../img/returnWhite.png);
    background-size: 24px 24px;
}
.left{
	float: left;
	line-height:50px;
	font-size:14px;
	margin-left: 5%;
	width:calc(60px + 8vw);
}
.right{
	float:left;
	width:calc(80vw - 60px);
}
.inputType{
	width: 100%;
	height: 36px !important;
	margin:6px 0 0 0 !important;
	outline: none;
	border: none !important;
	font-size: 1.4em;
	padding:0px !important;}
.textareaType{
	width: 100%;
	height: 100px;
	outline: none;
	border: none;
	margin-top: 15px;
	font-size:1.6em;
	padding: 0 !important;
}
.textareaLeft{
	float: left;
	font-size:14px;
	margin-left: 5%;
	width:calc(60px + 8vw);
}
.address{
	float:left;
	width:calc(82vw - 60px);
}
.addressLeft{
	float: left;
	font-size:1.4em;
	color:#A9A9A9;
	line-height:50px;
}

.sureBtn{
	width:94%;
	height: 42px;
	background:#D13600;
	margin-left:3%;
	border: none;
	outline: none;
	border-radius:8px;
	font-size:1.8em;
	color:#FFFFFF;
	margin-top: 50px;
}
input::-webkit-input-placeholder,
textarea::-webkit-input-placeholder{
	color:#A9A9A9;
}
.mui-btn {
	font-size: 16px;
	padding: 8px;
	margin: 3px;
}
h5.mui-content-padded {
	margin-left: 3px;
	margin-top: 20px !important;
}
h5.mui-content-padded:first-child {
	margin-top: 12px !important;
}
.ui-alert {
	text-align: center;
	padding: 20px 10px;
	font-size: 16px;
}
#showCityPicker{
	border: none;
	outline: none;
	margin: 0 !important;
	padding:0 !important;
	height: 36px !important;
	margin-top: 7px !important;
	width: 100%;
	text-align: left;
	color:#A9A9A9;
	background: url(/resources/images/down.png) no-repeat scroll right center transparent;
	background-size: 20px 10px;
	outline: none;
	background-position:95% 13px;
}
.oneLine{
	height:50px;
	background:#fff;
	border-bottom:1px solid #F1F1F1;
}
