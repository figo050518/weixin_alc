package com.fcgo.weixin.dto;

import com.fcgo.weixin.persist.po.OrderItemPO;
import com.fcgo.weixin.persist.po.ProductImagePO;

/**
 * 蛋蛋子项 返回结果集
 * 
 * @author Ww
 */
public class OrderItemsDTO {
    // 订单子表对象
    private OrderItemPO orderItemPO;
    // 商品图片表对象
    private ProductImagePO productImagePO;

    // 是否申请收货
    private int isRefund;

    public OrderItemPO getOrderItemPO() {
        return orderItemPO;
    }

    public void setOrderItemPO(OrderItemPO orderItemPO) {
        this.orderItemPO = orderItemPO;
    }

    public ProductImagePO getProductImagePO() {
        return productImagePO;
    }

    public void setProductImagePO(ProductImagePO productImagePO) {
        this.productImagePO = productImagePO;
    }

    public int getIsRefund() {
        return isRefund;
    }

    public void setIsRefund(int isRefund) {
        this.isRefund = isRefund;
    }

}
