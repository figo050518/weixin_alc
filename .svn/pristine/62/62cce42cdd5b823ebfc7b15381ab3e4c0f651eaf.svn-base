*{
	padding: 0;
	margin: 0;
}
.clear{
	clear: both;
}
body{
	font-family: helvetica,"microsoft yahei";
	color: #333;
	background: #FAFAFA;
	font-size: 62.5%;
}
.head{
	height:50px;
	background: #D13600;
	width: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 101;
}
.title{
	line-height: 50px;
	font-size: 16px;
	color: #FFFFFF;
	text-align: center;
}
.return{
	position: absolute;
	height: 40px;
	top: 6px;
	line-height: 40px;
	font-size: 16px;
	color: #fff;
}
.returnImg{
	position: relative;
    top: 5px;
    display: inline-block;
    margin-left: 3px;
    width: 24px;
    height: 24px;
    background-repeat: no-repeat;
    background-image: url(/resources/images/returnWhite.png);
    background-size: 24px 24px;
}

#addressDiv{
	width:100%;
	padding: 3vw 0;
	font-size: 14px;
}
.orderStatus{
	padding: 3vw 0;
	border-bottom: 1px solid #EBECEE;
	background:#fff;
	width: 100%;
}
.orderStatusImg{
	width:13vw;
	height:13vw;
}
.orderStatusTd{
	font-size: 4vw;
}
.sellerInfo{
	width: 100%;
	margin-top: 10px;
	background: #FFFFFF;
	border-bottom: 1px solid #EBECEE;
	border-top: 1px solid #EBECEE;
	height: 12vw;
	line-height: 12vw;
	font-size: 14px;
}
.nickImg{
	position: relative;
    top: 1vw;
    display: inline-block;
    width: 5vw;
    height: 5vw;
    background-repeat: no-repeat;
    background-image: url(/resources/images/nickNameImg.png);
    background-size: 5vw 5vw;
    margin-right: 2vw;
    margin-left: 3vw;
}
.oneGoods{
	border-bottom: 1px solid #EBECEE;
}
.goodsInfo{
	float: left;
	width: 75vw;
	border-bottom: 1px solid #EBECEE;
	padding-bottom: 1vw;
}
.goodsImgDiv{
	width:22vw;
	margin-left:3vw;
	float: left;
	height:22vw;
}
.goodsImg{
	width: 100%;
	display:block;
	height:100%;
}
.goodsInfoRight{
	width:18vw;
	margin-right: 0%;
	float: right;
	height:22vw;
	position: relative;
	margin-right:3vw;
}
.goodsInfoMiddle{
	width: 45vw;
	height:22vw;float: left;
	margin-left:3vw;
	position: relative;
}
.goodsName{
	width:100%;
	-webkit-line-clamp: 2; 
	text-overflow: ellipsis; 
	-o-text-overflow: ellipsis; 
	overflow: hidden;
	-webkit-box-orient: vertical;
	display: -webkit-box;
	text-overflow: -o-ellipsis-lastline;
	word-break:break-all;
	font-size: 14px;
	margin-top: 1vw;
}
.goodsDescribeDiv{
	width:100%;
	position: absolute;
	left:0;
	bottom:1vw;
	
}
.goodsDescribe{
	width:100%;
	-webkit-line-clamp: 2; 
	text-overflow: ellipsis; 
	-o-text-overflow: ellipsis; 
	overflow: hidden;
	-webkit-box-orient: vertical;
	display: -webkit-box;
	text-overflow: -o-ellipsis-lastline;
   word-break:break-all;
   color:#999999;
    font-size:12px;	
  line-height: 14px;
}
.goodsDiv{
	width: 100%;
	margin-top: 10px;
	position: relative;
}
.goodsCount{
	position:absolute;
	right: 0px;
	bottom: 0px;
	font-size:12px;
}
.btn{
	background: #FFD900;
	padding:5px;
	border: none;
	outline: none;
	font-size:13px;
	position:absolute;
	right:10%;
	top:50%;
	margin-top:-12px;
}
.goodsOperation{
	height: 12vw;
	background:#FFFFFF;
}
.goodsStatus{
	float: left;
	margin-left: 3vw;
	font-size: 14px;
	color: #FBBE88;
	border: 1px solid #FBBE88;
	box-sizing: border-box;
	height: 8vw;
	line-height: 8vw;
	margin-top: 2vw;
	padding: 0 10px;
	border-radius: 5px;
}
.goodsBtn{
	float: right;
	margin-right: 3vw;
	font-size: 14px;
	color: #FFFFFF;
	border: 1px solid #D13600;
	box-sizing: border-box;
	height: 8vw;
	margin-top: 2vw;
	padding: 0 10px;
	border-radius: 5px;
	background: #D13600;
}
.orderMoneyDiv{
	background: #FFFFFF;
	width: 97vw;
	margin-left: 3vw;
	font-size: 12px;
	line-height: 22px;
	padding: 1vw 0;
	border-bottom: 1px solid #EBECEE;
}
.freightText{
	float: left;
	color: #666666;
}
.freight{
	float: right;
	margin-right: 3vw;
	color: #666666;
}
.buyerMessageDiv{
	width: 97vw;
	margin-left: 3vw;
	padding: 2vw 0;
	font-size: 14px;
	line-height: 22px;
	border-bottom: 1px solid #EBECEE;
}
.buyerMessage{
	margin-right: 3vw;
	
}
.totleMoneyDiv{
	text-align: right;
	margin-right: 3vw;
	font-size: 14px;
	height: 12vw;
	line-height: 12vw;
}
.totleMoney{
	color: #FBBE88;
}
.totleCalculateDiv{
	background: #FFFFFF;
	padding: 3vw 0;
	margin-top: 10px;
	border-top: 1px solid #EBECEE;;
}
.totleCalculate{
	width: 100%;
	text-align: center;
	font-size: 14px;
}
.totleResult{
	width: 100%;
	text-align: center;
	font-size: 16px;
	color: #FBBE88;
	margin-top: 1.5vw;
}
.floor{
	position: fixed;
	width: 100%;
	height: 15vw;
	left: 0;
	bottom: 0;
	border-top: 1px solid #EBECEE;
	text-align: center;
	background: #FFFFFF;
}
.floorBtn{
	height: 10vw;
    width: 50vw;
    outline: none;
    border: none;
    box-sizing: border-box;
    margin-top: 2.5vw;
    border-radius: 3px;
    font-size: 16px;
    background: #FFFFFF;
    border: 1px solid #EBECEE;
    margin-left: 4vw;
    color: #333;
}

