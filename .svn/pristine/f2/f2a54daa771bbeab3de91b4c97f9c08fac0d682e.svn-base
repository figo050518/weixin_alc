*{
	padding: 0;
	margin: 0;
}
.clear{
	clear: both;
}
body{
	font-family: helvetica,"microsoft yahei";
	color: #333;
	background: #FAFAFA;
}
.head{
	height:50px;
	background: #D13600;
	width: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 101;
}
.headNav{
	width: 50%;
	height: 30px;
	box-sizing: border-box;
	border: 1px solid #FFFFFF;
	border-radius: 4px;
	position:absolute;
    top:50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    line-height: 30px;
    font-size: 14px;
}
.headNavCover{
	width: 100%;
	height: 100%;
	border-radius: 4px;
	padding: 1px;
	position:absolute;
    top:-1px;
    left: -1px;
    background: #D13600;
    opacity: 0.5;
    display: none;
}
.headNavL{
	width: 50%;
	height: 100%;
	background: #fff;
	float: left;
	text-align: center;
	border-radius: 3px 0 0 3px !important;
	color: #D13600;
}
.headNavR{
	width: 50%;
	height: 100%;
	float: left;
	background: #D13600;
	border-radius:0 3px 3px 0 !important;
	text-align: center;
	color: #fff;
}
.return{
	position: absolute;
	height: 40px;
	top: 6px;
	line-height: 40px;
	font-size: 16px;
	color: #fff;
}
.returnImg{
	position: relative;
    top: 5px;
    display: inline-block;
    margin-left: 3px;
    width: 24px;
    height: 24px;
    background-repeat: no-repeat;
    background-image: url(/resources/images/returnWhite.png);
    background-size: 24px 24px;
}
.tapDiv{
	width: 100%;
	height: 10vw;
	line-height: 10vw;
	font-size: 3vw;
	text-align: center;
	position: fixed;
	top: 50px;
	left: 0;
	box-sizing: border-box;
	z-index: 101;
	background:#FAFAFA;
}
.tap{
	width: 100%;
	height: 100%;
	box-sizing: border-box;
	border-bottom: 1px solid #EBECEE;
}
#tap1{
	border-bottom:2px solid #D13600;
}
.goodsList{
	border-bottom: 1px solid #EBECEE;
}
.oneGoods{
	width: 100%;
	position: relative;
}
.goodsImgDiv{
	width: 22vw;
	margin-left: 3vw;
	float: left;
	height: 28vw;
}
.goodsImg{
	width: 22vw;
	height: 22vw;
	display: block;
	border-radius: 3px;
	margin-top: 3vw;
}
.goodsInfo{
	margin-left: 3vw;
	width: 72vw;
	height: 28vw;
	float: left;
	position: relative;
	border-bottom: 1px solid #EBECEE;
}
.goodsName{
	margin-top: 4vw;
	width: 100%;
	-webkit-line-clamp: 2;
	text-overflow: ellipsis;
	overflow: hidden;
	-webkit-box-orient: vertical;
	display: -webkit-box;
	font-size: 4vw;
}
.goodsInfoFloor{
	position: absolute;
	left: 0;
	bottom:4vw;
	font-size: 3.5vw;
}
.price{
	color: #D13600;
}
.blank{
	width: 100%;
	height:calc(50px + 10vw);
}
.orderInfoDiv{
	width: 95vw;
	margin-left: 5vw;
	color: #999999;
	font-size: 3.5vw;
	height: 8vw;
	line-height: 8vw;
	border-bottom: 1px solid #EBECEE;;
}
.orderNum{
	float: left;
}
.orderTime{
	float: right;
	margin-right: 5vw;
}
.orderState{
	float: right;
	margin-right: 5vw;
	color: #FBBE88;
	font-size: 4vw;
}
.blankState{
	width: 100vw;
	height: 100vh;
	position: absolute;
	top: 0;
	left: 0;
	background: #FAFAFA;
	z-index: 100;
}
.blankImgDiv{
	width: 80vw;
	margin-left: 10vw;
	margin-top: 40vw;
}
.blankImg{
	width: 100%;
	display: block;
}
.blankText{
	width: 100%;
	text-align: center;
	font-size: 16px;
	margin-top: 5vw;
	color: #B0B9C2;
}
.blankBtnDiv{
	width: 100%;
	height: 40px;
	position: relative;
	margin-top: 8vw;
	text-align: center;
}
.blankBtn{
	outline: none;
	border: none;
	font-size: 16px;
	border-radius: 20px;
	padding: 0 30px;
	color: #FFFFFF;
	background: #D13600;
	height: 40px;
}
