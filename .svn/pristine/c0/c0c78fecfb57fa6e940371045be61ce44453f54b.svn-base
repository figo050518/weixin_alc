body{
	background: #FAFAFA;padding: 0;margin: 0;
	color: #333;
	font-family: helvetica,"microsoft yahei";

}
.titleTable{
	height: 50px;
	width: 100%;
}
.titleTable td{
	background:#D13600;
	padding:0 2vw;
	color: #fff;
	font-size: 16px;
}
ul{
	padding: 0;
	margin: 0;
	
}
a{
	color: blue;
	text-decoration: none;
	font-size: 3.3vw;
}
p{
	padding: 0;
	margin: 1vw 0;
}
.TXtable th{
	font-size: 14px;
	text-align: center;
	font-weight: 400;
	padding: 3vw 0;
}
.TXtable td{
	background: #fff;
	color:#333;
	font-size: 14px;
	text-align: center;
	padding: 3vw 0;
	border-bottom: 1px solid #EBECEE;
}