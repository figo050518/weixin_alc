
body{
	background: #FAFAFA;
	padding: 0;
	margin: 0;
	color: #333;
	font-family: helvetica,"microsoft yahei";

}
.titleTable{
	height: 50px;
	width: 100%;
}
.titleTable td{
	background:#D13600;
	padding:0 2vw;
	color: #fff;
	font-size: 16px;
}

.user{
	margin-top:5vw;
	width: 90vw;
	padding: 0 5vw;
	height: 15vw;
	background: #fff;
	font-size:14px;
	border-bottom: 1px solid #f5f5f5;
}
.user div{
	width: 15vw;
	text-align: center;
	float: left;
	line-height: 15vw;
}
.user .userName{
	width: 70vw;
	float: left;
	height: 15vw;
	border: 0;
	outline: 0;
	padding: 0;
	
}
.psWord{
	width: 90vw;
	padding: 0 5vw;
	height: 15vw;
	background: #fff;
	font-size:14px;
}
.psWord div{
	width: 15vw;
	text-align: center;
	float: left;
	line-height: 15vw;
}
.psWord .psWordNum{
	width: 70vw;
	float: left;
	height: 15vw;
	border: 0;
	outline: 0;
	padding: 0;
}
.signBTN{
	width: 90vw;
	height: 11vw;
	margin-left: 5vw;
	border:1px solid #D13600;
	outline: 0;
	background: #D13600;
	color:#fff;
	font-size: 14px;
	border-radius:2px;
	margin-top:5vw;
}
.otherBtn{
	width: 90vw;
	height: 10vw;
	margin-left: 5vw;
	position: relative;
	line-height: 10vw;
}
.forgetPsd{
	text-decoration: none;
	color:#0000ff;
	font-size: 12px;
}
