package com.fcgo.weixin.dto;

import java.math.BigDecimal;

import com.fcgo.weixin.persist.po.UserInfoPO;

/**
 * 客户关系
 * 
 * @author Ww
 */
public class SellerCustomerDTO {
    // 客户ID
    private int userId;
    // 总计消费金额
    private BigDecimal sumMoey;
    // 用户信息
    private UserInfoPO userInfoPO;

    public int getUserId() {
        return userId;
    }

    public void setUserId(int userId) {
        this.userId = userId;
    }

    public BigDecimal getSumMoey() {
        return sumMoey;
    }

    public void setSumMoey(BigDecimal sumMoey) {
        this.sumMoey = sumMoey;
    }

    public UserInfoPO getUserInfoPO() {
        return userInfoPO;
    }

    public void setUserInfoPO(UserInfoPO userInfoPO) {
        this.userInfoPO = userInfoPO;
    }

}
