*{
	padding: 0;
	margin: 0;
}
.clear{
	clear: both;
}
body{
	font-family: helvetica,"microsoft yahei";
	color: #333;
	background: #FAFAFA;
}
.head{
	height:50px;
	background: #D13600;
	width: 100%;
	position: relative;
}
.headNav{
	width: 50%;
	height: 30px;
	box-sizing: border-box;
	border: 1px solid #FFFFFF;
	border-radius: 4px;
	position:absolute;
    top:50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    line-height: 30px;
    font-size: 14px;
}
.headNavCover{
	width: 100%;
	height: 100%;
	border-radius: 4px;
	padding: 1px;
	position:absolute;
    top:-1px;
    left: -1px;
    background: #D13600;
    opacity: 0.5;
    display: none;
}
.headNavL{
	width: 50%;
	height: 100%;
	background: #fff;
	float: left;
	text-align: center;
	border-radius: 3px 0 0 3px !important;
	color: #D13600;
}
.headNavR{
	width: 50%;
	height: 100%;
	float: left;
	background: #D13600;
	border-radius:0 3px 3px 0 !important;
	text-align: center;
	color: #fff;
}
.return{
	position: absolute;
	height: 40px;
	top: 6px;
	line-height: 40px;
	font-size: 16px;
	color: #fff;
}
.returnImg{
	position: relative;
    top: 5px;
    display: inline-block;
    margin-left: 3px;
    width: 24px;
    height: 24px;
    background-repeat: no-repeat;
    background-image: url(/resources/images/returnWhite.png);
    background-size: 24px 24px;
}
.headIcon{
	width: 25px;
	height: 25px;
	margin: 7.5px;
}
.complete{
	color: #FFFFFF;
	font-size: 16px;
	line-height: 40px;
	display: none;
}
.searchDiv{
	position: absolute;
	width: 40px;
	height: 40px;
	top: 5px;
	right: 5vw;
	text-align: center;
}
.listNavDiv{
	height:42px;
	width: 100%;
	font-size: 14px;
}
.listNav{
	height: 40px;
	line-height: 40px;
	background:#F7F9FA;
}
.tap{
	float: left;
	width: 50%;
	text-align: center;
}
.inSale{
	border-bottom: 2px solid #D13600;
}
.haveShelves{
	border-bottom: 2px solid #F7F9FA;
}
.goodsList{
	border-bottom: 1px solid #EBECEE;
}
.oneGoods{
	width: 100%;
	position: relative;
}
.goodsImgDiv{
	width: 22vw;
	margin-left: 3vw;
	float: left;
	height: 28vw;
}
.goodsImg{
	width: 22vw;
	height: 22vw;
	display: block;
	border-radius: 3px;
	margin-top: 3vw;
}
.goodsInfo{
	margin-left: 3vw;
	width: 72vw;
	height: 28vw;
	float: left;
	position: relative;
	border-bottom: 1px solid #EBECEE;
}
.goodsName{
	margin-top: 4vw;
	width: 100%;
	-webkit-line-clamp: 2;
	text-overflow: ellipsis;
	overflow: hidden;
	-webkit-box-orient: vertical;
	display: -webkit-box;
	font-size: 4vw;
}
.goodsInfoFloor{
	position: absolute;
	left: 0;
	bottom:4vw;
	font-size: 3.5vw;
}
.price{
	color: #D13600;
}
.operationIcon{
	width: 30px;
	height: 30px;
}
.operationDiv{
	position: absolute;
	width: 30px;
	height: 30px;
	bottom: 3vw;
	right: 5vw;
}
.operationList{
	position: absolute;
	right: 3vw;
	bottom: calc(3vw + 35px);
}
.navigation {
    background: #000000;
    opacity: 0.8;
    border-radius:5px;
    width: 90vw;
    color: #FFFFFF;
    padding:10px 0 5px 0;
}
.operationTab{
	width: 100%;
}
.floorNav{
	width: 100%;
	text-align: center;
	font-size: 0;
}
.operationImg{
	width: 20px;
	height: 20px;
}
.operationName{
	width: 100%;
	text-align: center;
	font-size: 12px;
	margin-top:5px;
}
.sanjiao {
    width: 0;
    height: 0;
    border-width: 7px 7px 0px 7px;
    border-style: solid;
    border-color: #000000 transparent transparent transparent;
    opacity: 0.8;
    margin-left: 81.5vw;
}
.floor{
	width: 100%;
	height: 15vw;
	border-top: 1px solid #EBECEE;
	position: fixed;
	bottom: 0;
	left: 0;
}
.floorBtn{
	height: 10vw;
	width: 40vw;
	outline: none;
	border: none;
	box-sizing: border-box;
	margin-top: 2.5vw;
	border-radius: 3px;
	font-size: 16px;
}
.addGoods{
	background: #D13600;
	border: 1px solid #D13600;
	margin-left: 8vw;
	color: #fff;
}
.management{
	background: #FFFFFF;
	border: 1px solid #EBECEE;
	margin-left: 4vw;
	color: #333;
}
.checkGoods{
	width: 15vw;
	float: left;
	height:28vw;
	position: relative;
	display: none;
}
.checkboxDiv{
	width: 20px;
	height: 20px;
	background: #FFFFFF;
	border-radius: 50%;
	outline: none;
	position:absolute;
    top:50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
}
.opaction{
	-webkit-appearance: none;
	position: relative;
	width: 20px;
	height: 20px;
	background:#fff;
	border: none;
	display: inline-block;
	outline: none;
	z-index: 10000;
	border: 1px solid #D13600;
	border-radius: 50%;
}
.opaction:checked{
	background:#D13600;
}
.opaction:checked::before{
	opacity: 1;
	content: '';
	position: absolute;
	width: 8px;
	height: 5px;
	top: 4px;
	left: 4px;
	border: 2px solid #fff;
	border-top: none;
	border-right: none;
	-webkit-transform:rotate(-45deg);
}

.flooroPeration{
	width: 100%;
	height: 100%;
	display: none;
}
.checkAllDiv{
	float: left;
}
.floorPerationImg{
	width: 50px;
	height: 20px;
	text-align: center;
	font-size: 0;
	margin-top: 3px;
}
.checkboxAll{
	width: 20px;
	height: 20px;
	background: #FFFFFF;
	border-radius: 50%;
	outline: none;
}
.floorPerationName{
	width: 50px;
	text-align: center;
	font-size: 12px;
	height: calc(15vw - 23px);
	line-height: calc(15vw - 23px);
}
.floorImg{
	width: 20px;
	height: 20px;
}
