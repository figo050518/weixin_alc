*{
	padding: 0;
	margin: 0;
}
.clear{
	clear: both;
}
body{
	width:100%;
	height: 100%;
	font-size:62.5%;
	font-family: "微软雅黑";
	background:#F5F5F5;
}
.head{
	height:50px;
	background: #D13600;
	width: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 101;
}
.title{
	line-height: 50px;
	font-size: 16px;
	color: #FFFFFF;
	text-align: center;
}
.return{
	position: absolute;
	height: 40px;
	top: 6px;
	line-height: 40px;
	font-size: 16px;
	color: #fff;
}
.returnImg{
	position: relative;
    top: 5px;
    display: inline-block;
    margin-left: 3px;
    width: 24px;
    height: 24px;
    background-repeat: no-repeat;
    background-image: url(/resources/images/returnWhite.png);
    background-size: 24px 24px;
}
.releaseDiv{
	height: 40px;
	position: absolute;
	top: 5px;
	right: 3vw;
	line-height: 40px;
	font-size: 16px;
	color: #FFFFFF;
}

.payTypeDiv{
	background: #fff;
	height:50px;
}
.portrait{
	background: #fff;
	height: 80px;
}
#fileInputContainer{
    position:relative;
    width: 100%;
    height: 80px;
    z-index:10;
    margin-top:60px;
}
.file{
    height:80px;
    width: 100%;
    position:absolute;
    right:0;
    top:0;
    opacity: 0;
    filter:alpha(opacity=0);
    cursor:pointer;
    z-index: 100;
}
.portrait{
	background: #fff;
	height: 80px;
}
#magnifying{
   position: fixed;
   position: absolute;
   width: 100%;
   background-color:rgba(0,0,0,0);
   z-index:1003; 
   display:none;
  top:50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
}
.overlay{
	position:fixed;
	_position:absolute;
	width:100%;
	height:100%;
	left:0;
	top:0; 
	display:none;
    background-color:rgba(0,0,0,0.7);
    z-index:1002; 
}

.noBind{
	width: 100%;
	height: 100%;
	position: relative;
	
}
.noBindTop{
	width: 100%;
	height: 12vw;
	background: #fff;
	border-bottom:2px solid #f0f0f0;
}
.noBindTop span{
	width: 90vw;
	left: 5vw;
	position: absolute;
	line-height: 12vw;
	color:#666;
	font-size: 5vw;
	
}
.noBindcontent{
	position: relative;
	width: 100%;
	background: #f5f5f5;
}
.userImg{
	width: 100%;
	height: 35vw;
	position: relative;
	text-align: center;
	
}
.userImg img{
	margin-top:7.5vw;
	width: 20vw;
	height:20vw;
	border-radius: 10vw;
}
.defaltNum{
	width: 85vw;
	position: relative;
	margin-left: 5vw;
	padding-left: 5vw;
	height: 11vw;
	background: #eee;
	line-height: 11vw;
	border-radius: 5.5vw;
}
.contentinfo{
	width: 85vw;
	position: relative;
	margin-left: 5vw;
	padding-left: 5vw;
	margin-top:30px;
}
.phoneNum{
	width: 80vw;
	position: relative;
	margin-left: 5vw;
	padding-left: 5vw;
	height: 11vw;
	line-height: 11vw;
	border-radius:5.5vw;
	border:none;
	outline: none;
	margin-top:15px;
	background: #fff;
	padding-right: 5vw;
}
.address{
	width: 80vw;
	position: relative;
	margin-left: 5vw;
	padding-left: 5vw;
	height: 11vw;
	line-height: 11vw;
	border-radius: 5.5vw;
	border:none;
	outline: none;
	margin-top:20px;
	background: #fff;
	padding-right: 5vw;
}
.navigation{
	position: absolute;
	right:5px;
	top: 50px;
	background: #000000;
   	opacity: 0.8;
   	z-index: 10;
   	padding: 0 10px;
   	border-radius: 6px;
}
#test1 {
 	width:0;
       height:0;           
       border-width:0px 7px 10px 7px;           
       border-style:solid;            
       border-color:transparent transparent #000000 transparent;
       opacity: 0.7;
       position: absolute;  
       right: 22px;
       top: 40px;
       z-index: 1000;
}
.edit img{
	width:100% !important;
	height:100% !important;
	display:block;
}
