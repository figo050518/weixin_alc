*{
	padding: 0;
	margin: 0;
}
.clear{
	clear: both;
}
body{
	font-family: helvetica,"microsoft yahei";
	color: #333;
	background: #FAFAFA;
	font-size: 62.5%;
}
.overHiden{
	overflow: hidden;
	height: 100%;
}
.head{
	height:50px;
	background: #D13600;
	width: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index:101;
}
.head2{
	height:50px;
	background: #D13600;
	width: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index:111;
}
.nickNameDiv{
	width: 100%;
	height: 50px;
	line-height: 50px;
	text-align: center;
	font-size: 16px;
	color: #fff;
}
.return{
	position: absolute;
	height: 40px;
	top: 6px;
	line-height: 40px;
	font-size: 16px;
	color: #fff;
}
.returnImg{
	position: relative;
    top: 5px;
    display: inline-block;
    margin-left: 3vw;
    width: 24px;
    height: 24px;
    background-repeat: no-repeat;
    background-image: url(/resources/images/returnWhite.png);
    background-size: 24px 24px;
}

.selfInfo{
	width:100%;
	position: relative;
	background: #fff;
}
.selfInfo-top{
	width:96vw;
	position: relative;
	height: 18vw;
	padding: 2vw;
	border-bottom: 1px solid #EBECEE;
}
.selfInfo-top img{
	width: 18vw;
	height: 18vw;
	float: left;
}
.selfInfo-topInfo{
	width: 75vw;
	height: 18vw;
	float:left;
	padding-left: 2vw;
}
.selfInfo-topInfo>span{
	display:block;
	
	
}
.selfInfo-topInfo span:nth-child(1){
	font-size: 14px;
	line-height: 8vw;
}
.selfInfo-topInfo span:nth-child(2){
	font-size: 12px;
	color: #999;
}
.selfInfo-topInfo span:nth-child(2) span{
	font-size: 12px;
}
.Setremarks{
	width:66px;
	height:25px;
	font-size: 12px;
	position: absolute;
	right: 10vw;
	bottom: 2vw;
	outline: none;
	border: 1px solid #EBECEE;
	background:#Fff;
	color: #666;
	
}
.td1,.td3{
	width: 54vw;
	text-align: center;
	padding: 2vw 0;
}
.td2{
	width: 2vw;
	text-align: center;
	padding: 2vw 0;
	color:#999;
	font-size: 20px;
}
.td2 div{
	width: 1px;
	height: 12vw;
	border-left: 1px solid #EBECEE;
}
.total{
	font-size: 12px;
	color:#666;
}
.totalMoney,.totalNum{
	font-size: 16px;
	line-height: 16px;
}
.content{
	width: 100%;
	position: relative;
	background: #FFFFFF;
	margin-top: 20px;
}
.content_item{
	width: 100%;
	height: 15vw;
	position: relative;
	border-bottom: 1px solid #EBECEE;
}
.content_item img:nth-child(1){
	width: 7vw;
	height: 7vw;
	position: absolute;
	left: 3vw;
	top: 4vw;
}
.content_item .item_tit{
	width: 40vw;
	height: 7vw;
	font-size: 14px;
	position: absolute;
	left: 12vw;
	line-height:15vw;
}
.nextIcon{
	position: absolute;
	right: 3vw;
	height: 5vw;
	top:5vw;
}
.num{
	width: 10vw;
	right: 8vw;
	position: absolute;
	font-size: 14px;
	text-align: right;
	line-height: 15vw;
}
